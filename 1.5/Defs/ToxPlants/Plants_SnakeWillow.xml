<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <ThingDef ParentName="BMT_PollutedLands_DeciduousTreeBase">
	<defName>BMT_Plant_SnakeWillow</defName>
	<label>snake willow</label>
	<description>A willow tree that thrives in swamps riddled with pollution and water that would poison most other plants. It does not provide much usable wood.</description>
	<minifiedDef IsNull="True" />
	<statBases>
	  <MaxHitPoints>120</MaxHitPoints>
	  <Nutrition>2.0</Nutrition>
	  <Mass>100</Mass>
	</statBases>
	<graphicData>
	  <texPath>BMT_PollutedLands/Things/SnakeWillow/SnakeWillow</texPath>
	  <graphicClass>Graphic_Random</graphicClass>
	  <shadowData>
		<volume>(0.24, 0.3, 0.15)</volume>
		<offset>(0,0,-0.38)</offset>
	  </shadowData>
	</graphicData>
	<fillPercent>0.25</fillPercent>
	<plant>
	  <lifespanDaysPerGrowDays>40</lifespanDaysPerGrowDays>
	  <fertilityMin>0.05</fertilityMin>
	  <fertilitySensitivity>0</fertilitySensitivity>
	  <growDays>13</growDays>
	  <harvestWork>110</harvestWork>
	  <harvestYield>10</harvestYield>
	  <harvestMinGrowth>0.2</harvestMinGrowth>
	  <topWindExposure>0.08</topWindExposure>
	  <wildClusterRadius>-1</wildClusterRadius>
	  <treeLoversCareIfChopped>true</treeLoversCareIfChopped>
	  <leaflessGraphicPath>BMT_PollutedLands/Things/ToxPlants/SnakeWillow/SnakeWillow_Leafless</leaflessGraphicPath>
	  <harvestedThingDef>ToxWood</harvestedThingDef>
	</plant>
  </ThingDef>

</Defs>
