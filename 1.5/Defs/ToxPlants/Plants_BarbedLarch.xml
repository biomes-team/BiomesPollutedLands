<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <ThingDef ParentName="BMT_PollutedLands_DeciduousTreeBase">
	<defName>BMT_Plant_BarbedLarch</defName>
	<label>barbed larch tree</label>
	<description>A colder-biome tree known for its barbed needles. Much like a porcupine's quills, it's unpleasant to get poked by as the needles have backwards facing barbs.</description>
	<graphicData>
	  <texPath>BMT_PollutedLands/Things/ToxPlants/BarbedLarch/BarbedLarch</texPath>
	  <graphicClass>Graphic_Single</graphicClass>
	  <shadowData>
		<volume>(0.15, 0.3, 0.1)</volume>
		<offset>(-0.03,0,-0.44)</offset>
	  </shadowData>
	</graphicData>
	<statBases>
	  <Beauty>2</Beauty>
	  <BeautyOutdoors>2</BeautyOutdoors>
	  <MaxHitPoints>200</MaxHitPoints>
	  <Nutrition>2.0</Nutrition>
	  <Mass>150</Mass>
	</statBases>
	<ingestible />
	<plant>
	  <growDays>20</growDays>
	  <harvestWork>400</harvestWork>
	  <harvestYield>10</harvestYield>
	  <!-- <immatureGraphicPath>Things/Plant/TreeBirch_Immature</immatureGraphicPath> -->
	</plant>
  </ThingDef>

</Defs>
