<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <ThingDef ParentName="BMT_PollutedLands_PlantBase">
    <defName>BMT_Plant_ToxicIvy</defName>
    <label>toxic ivy</label>
    <description>Toxic ivy is generally considered a weed to those in polluted areas, as it survives virtually anything thrown at it, barring fire.</description>
    <statBases>
      <MaxHitPoints>85</MaxHitPoints>
      <Flammability>1.3</Flammability>
      <Nutrition>0.50</Nutrition>
    </statBases>
    <graphicData>
	  <texPath>BMT_PollutedLands/Things/ToxPlants/ToxicIvy/ToxicIvy</texPath>
      <graphicClass>Graphic_Random</graphicClass>
    </graphicData>
    <hideAtSnowDepth>0.5</hideAtSnowDepth>
    <ingestible />
    <selectable>false</selectable>
    <plant>
      <growDays>2.5</growDays>
      <harvestWork>40</harvestWork>
      <maxMeshCount>9</maxMeshCount>
      <visualSizeRange>0.4~0.6</visualSizeRange>
      <topWindExposure>0.4</topWindExposure>
      <wildOrder>1</wildOrder>
    </plant>
  </ThingDef>

</Defs>
